<!DOCTYPE html>
<html>
  <%- include('./partials/head.ejs', {title}) %>
  <body>
    <div class="container-fluid">
      <div class="row">
        <%- include('./partials/sidebar.ejs') %>
        <div class="col-sm-10">
          <h1><%= title %></h1>
          <ul>
            <% author_list.forEach(author => { %>
            <li><a href=<%= author.url %>><%= author.name %></a> | (<%= author.date_of_birth_formatted %> - <%= author.date_of_death_formatted %> )</li>
            <% }) %>
          </ul>
        </div>
      </div>
    </div>
  </body>
</html>
